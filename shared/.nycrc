{
    "extends": "@istanbuljs/nyc-config-typescript",
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "all": true,
    "check-coverage": false,
    "reporter": [ "text", "html" ],
    "include": [ "src/**/*.ts" ],
    "exclude": [ 
        "**/test/src/**/*.ts",
        "**/types/**/*.d.ts"
    ],
    "report-dir": "../coverage/shared/nyc",
    "temp-dir": "../.nyc_output/shared",
    "useSpawnWrap": false
}